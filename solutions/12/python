e=enumerate
g={x+y*1j:c for y,l in e(open(0)) for x,c in e(l)if" "<c}
v={e}
x=y=0
k=0,1,-1,1j,-1j
for z,c in g.items():
 a=0;r={z}-v
 while r!=a:a=r;r={x+d for x in r for d in k if x+d in g if g[x+d]==c}
 v|=r
 P={(p,d)for d in k[1:] for p in r if 1-(p+d in r)};x+=len(r)*len(P);y+=len(r)*len(P-{(p+d*1j,d)for p,d in P})
print(x,y)