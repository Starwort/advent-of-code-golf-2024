from itertools import*
g=*open(0),;a=range(140)
p={*product(a,a)}
r,d=[],set()
for x,y in p:
 if(x,y)in d:continue
 c,o=set(),{(x,y)}
 while o:c,o=c|o,{(X,Y)for(x,y)in o for(X,Y)in{(x+1,y),(x-1,y),(x,y+1),(x,y-1)}if(X,Y)in p and g[Y][X]==g[y][x]}-(c|o)
 d|=c;r+=c,
j=k=0
for u in r:
 a=len(u)
 for _ in' '*4:e=sorted((x,y)for x,y in u if(x+1,y)not in u);s=1+sum(not(x1==x2 and y1+1==y2)for(x1,y1),(x2,y2)in zip(e,e[1:]));u={(-y,x)for x,y in u};j+=a*len(e);k+=a*s
print(j,k)