from itertools import*
l,m=open(0).read(),142
S=l.find('S')
D={S:0}
t=[S]
for p in t:
 for n in p-1,p+1,p-m,p+m:
  if'#'!=l[n]and n not in D:D[n]=D[p]+1;t+=n,
a=b=0
D={(c%m,c//m):D[c] for c in D}
for(p,i),(q,j)in combinations(D.items(),2):
 d=abs(p[0]-q[0])+abs(p[1]-q[1])
 if j-i-d>99:a+=d<3;b+=d<21
print(a,b)