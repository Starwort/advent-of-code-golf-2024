from heapq import*
L=open(0).read()
*q,d=(0,0),{}
v,*t=set(),(19739,1,-1)
b=9e9
f,l=heappush,len
while q:
 S,i=heappop(q);p,u,_=t[i]
 if d.get((p,u),b)<S:continue
 d[p,u]=S
 if p==281:
  b,A,_,C=S,*t[i]
  while-1!=C:v|={A};A,_,C=t[C];v|={A}
 if'#'<L[p+u]:f(q,(S+1,l(t)));t+=(p+u,u,i),
 for D in-1,1:f(q,(S+1000,l(t)));t+=(p,D*142//u,i),
print(b,l(v))