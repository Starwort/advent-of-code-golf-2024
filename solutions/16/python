import heapq as h
L=open(0).read()
q=[(0,0)]
d={}
t=[(19739,1,-1)]
v=set()
b=0
f,l=h.heappush,len
while q:
 S,i=h.heappop(q);p,u,_=t[i]
 if(p,u)in d and d[(p,u)]<S:continue
 d[(p,u)]=S
 if p==281and(b<1or S==b):
  b,c=S,t[i]
  while c[2]!=-1:v.add(c[0]);c=t[c[2]]
 if L[p+u]!='#':f(q,(S+1,l(t)));t+=[(p+u,u,i)]
 for D in 1,-1:f(q,(S+1000,l(t)));t+=[(p,D*142//u,i)]
print(b,l(v))