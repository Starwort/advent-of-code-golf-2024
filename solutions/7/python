from operator import*
a=b=0;c=lambda x,y:int(str(x)+str(y))
for l in open(0):
 x=l.split();x[0]=x[0][:-1];x,*ys=map(int, x);S=T=[ys[0]]
 for y in ys[1:]:S = [op(s,y) for s in S for op in (add,mul)]
 for y in ys[1:]:T = [op(s,y) for s in T for op in (add,mul,c)]
 if x in S:a+=x
 if x in T:b+=x
print(a,b)