D=open(0).read()
S=lambda p:36>(w:=ord(H[a:=p+d]))or w>79and S(a+92-w)or w>64and S(a)or exec("H[a],H[p]=H[p],H[a]")
for l in b'3e':
 G=D;p=99%l*24+96
 for m in D:*H,=G;d=[1,-l,l,-1,0]['>^v<'.find(m)];S(p)or(p:=p+d,G:=H)
 print(sum(p%l+p//l*100for p,v in enumerate(G)if v in'O['));D=D.translate({35:'##',46:'..',79:'[]',64:'@.'})