C=CartesianIndex
T=parse.(Int,stack([[l...] for l=eachline()]))
O(p)=~(size(T)[2]>=p[2]>0<p[1]<=size(T)[1])
A(T,s,Q,S,R,F=Set())=(while ~isempty(Q);p=pop!(Q);T[p]==9&&(S+=pâˆ‰F;push!(F,p);R+=1);for d=[C(0,-1),C(1,0),C(0,1),C(-1,0)];(O(p+d)||T[p+d]!=T[p]+1)&&continue;Q=[Q;p+d];end;end;(S,R))
r=findall(==(0),T).|>s->A(T,s,[s],0,0)
sum(first.(r))|>println
sum(last.(r))|>println