from functools import*
L=*open(0),
@cache
def F(s,k,r):
 if k>r:return len(s)
 x,y,m=[(2,3,"789456123_0A"),(2,0,"_^A<v>")][k>0];M={};i=t=0
 for v in m:M[v]=i%3,i//3;i+=1
 A,B=M["_"]
 for c in s:X,Y=M[c];a,b=X-x,Y-y;t+=F(("<"*-a+"v"*b+"^"*-b+">"*a)[::-(X==A)*(y==B)-(Y==B)*(x==A)|1]+"A",k+1,r);x,y=M[c]
 return t
print(*(sum(int((k:=l.strip())[:-1])*F(k,0,n)for l in L)for n in(2,25)))